{
  "name": "trsenna/WordPress-minimal",
  "description": "WordPress Minimal Project Template",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Thiago Senna",
      "homepage": "http://thremes.com.br"
    }
  ],
  "support": {
    "issues": "https://github.com/trsenna/WordPress-minimal/issues",
    "source": "https://github.com/trsenna/WordPress-minimal"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/trsenna/gozma14"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "johnpbloch/wordpress": ">=4.7",
    "wpackagist-plugin/regenerate-thumbnails": ">=2.2.6",
    "wpackagist-plugin/wordpress-importer": ">=0.6.3",
    "wpackagist-theme/twentytwelve": ">=2.2"
  },
  "require-dev": {
    "trsenna/gozma14": "1.0.0"
  },
  "extra": {
    "installer-paths": {
      "public_html/content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "public_html/content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "public_html/content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "public_html/wp"
  },
  "config": {
    "secure-http": false
  }
}
